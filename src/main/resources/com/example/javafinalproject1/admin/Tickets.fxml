<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafinalproject1.admin.controller.TicketController">
    <Label text="Ticket Management" style="-fx-font-size: 20px; -fx-padding: 10;"/>
    <TableView fx:id="table" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="colId" text="ID" />
            <TableColumn fx:id="colRoute" text="Route ID" />
            <TableColumn fx:id="colSeats" text="Seats Left" />
            <TableColumn fx:id="colPrice" text="Price" />
        </columns>
    </TableView>
    <Button text="Delete Selected Ticket" onAction="#deleteTicket" style="-fx-background-color: red; -fx-text-fill: white;"/>
</VBox>